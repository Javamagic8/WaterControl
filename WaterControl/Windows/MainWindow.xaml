                            </Canvas>
                            <TextBlock Name="textViewBtn" Text="Кузатув" Margin="0,0,0,5" FontSize="18" FontWeight="DemiBold" Foreground="#82898C"/>
                        </StackPanel>
                    </Button.Content>
                </Button>
                <Button x:Name="btnMonitoringPanel" Grid.Column="2" Click="btnMonitoringPanel_Click" HorizontalAlignment="Center" VerticalAlignment="Bottom" Background="{x:Null}" BorderThickness="0">
                    <Button.Content>
                        <StackPanel>
                            <Canvas Height="31.5" Width="31.5">
                                <Canvas>
                                    <Path x:Name="monitorbtncolor" Fill="#82898C" Data="M28,0H3.5A3.5,3.5,0,0,0,0,3.5V28a3.5,3.5,0,0,0,3.5,3.5H28A3.5,3.5,0,0,0,31.5,28V3.5A3.5,3.5,0,0,0,28,0ZM10.5,24.5H7V12.25h3.5Zm7,0H14V7h3.5Zm7,0H21v-7h3.5Z" />
                                </Canvas>
                            </Canvas>
                            <TextBlock Name="textMonitoringBtn" Text="Мониторинг" Margin="0,0,0,5" FontSize="18" FontWeight="DemiBold" Foreground="#82898C"/>
                        </StackPanel>
                    </Button.Content>
                </Button>
                <Button x:Name="btnInstructionPanel" Grid.Column="3" Click="btnInstructionPanel_Click" HorizontalAlignment="Center" VerticalAlignment="Bottom" Background="{x:Null}" BorderThickness="0">
                    <Button.Content>
                        <StackPanel>
                            <Canvas Height="25.5" Width="33">
                                <Canvas>
                                    <Path x:Name="commandbtncolor" Fill="#82898C" Data="M30,.75A17.87,17.87,0,0,0,24.75,0c-2.92,0-6.08.6-8.25,2.25C14.33.6,11.17,0,8.25,0S2.18.6,0,2.25v22A.81.81,0,0,0,.75,25c.15,0,.22-.07.37-.07a18.28,18.28,0,0,1,7.13-1.65c2.92,0,6.08.6,8.25,2.25a18,18,0,0,1,8.25-2.25,15.36,15.36,0,0,1,7.13,1.57.62.62,0,0,0,.37.08.81.81,0,0,0,.75-.75V2.25A10.05,10.05,0,0,0,30,.75ZM30,21a17.27,17.27,0,0,0-5.25-.75A18,18,0,0,0,16.5,22.5V5.25A18.15,18.15,0,0,1,24.75,3,17.59,17.59,0,0,1,30,3.75Z" />
                                </Canvas>
                            </Canvas>
                            <TextBlock Name="textCommandBtn" Text="Кўрсатма" Margin="0,0,0,5" FontSize="18" FontWeight="DemiBold" Foreground="#82898C"/>
                        </StackPanel>
                    </Button.Content>
                </Button>
            </Grid>
            <!--footer end-->
            <!--Page Content start-->
            <Border>
                <Border.Background>
                    <LinearGradientBrush StartPoint="0 0" EndPoint="0 1">
                        <LinearGradientBrush.GradientStops>
                            <GradientStop Offset="0.05" Color="#7FE5E5E5" />
                            <GradientStop Offset="1" Color="#FFFFFF" />
                        </LinearGradientBrush.GradientStops>
                    </LinearGradientBrush>
                </Border.Background>
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="9*"/>
                    </Grid.RowDefinitions>
                    <!--Navbar start-->
                    <Border Grid.Row="0" VerticalAlignment="Top">
                        <Border.Effect>
                            <DropShadowEffect>
                                <DropShadowEffect.Color>Gray</DropShadowEffect.Color>
                                <DropShadowEffect.Opacity>0.1</DropShadowEffect.Opacity>
                            </DropShadowEffect>
                        </Border.Effect>
                        <materialDesign:ColorZone
                    Mode="Inverted" BorderThickness="0" BorderBrush="Gray"
                    Background="#FFFFFF"
                    Foreground="Black"
                    CornerRadius="8"
                    Height="70">

                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="2*"/>
                                    <ColumnDefinition Width="2*"/>
                                    <ColumnDefinition Width="15*"/>
                                    <ColumnDefinition Width="9*"/>
                                    <ColumnDefinition Width="3*"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>

                                <ToggleButton Grid.Column="0"
                                    x:Name="ToggleButton"
                                    Click="ToggleButton_Click"
                                Foreground="#979797"
                                Style="{DynamicResource MaterialDesignHamburgerToggleButton}" />
                                <TextBlock Grid.Column="1"
                                    VerticalAlignment="Center"
                                     FontSize="20" HorizontalAlignment="Left">
                                    Menu
                                </TextBlock>
                                <Button Height="70" Grid.Column="2" Visibility="Hidden" BorderThickness="0" Background="{x:Null}" Click="backtocard_Click" Name="backtocard" IsEnabled="{Binding DownButton}">
                                    <Path Width="40" Height="60" Stretch="Fill" Fill="#979797" Data="M30 18L40 18L20 36L-7.86805e-07 18L10 18L10 -1.31134e-06L30 -4.37114e-07L30 18Z" RenderTransformOrigin="0.5,0.5">
                                        <Path.RenderTransform>
                                            <TransformGroup>
                                                <ScaleTransform/>
                                                <SkewTransform/>
                                                <RotateTransform Angle="90.516"/>
                                                <TranslateTransform/>
                                            </TransformGroup>
                                        </Path.RenderTransform>
                                    </Path>
                                </Button>
                                <TextBlock Grid.Column="3" x:Name="waterdDammName" Text="Андижон Вилояти" FontSize="35" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="SemiBold"/>
                                <Border Grid.Column="4" Margin="5" CornerRadius="5">
                                    <Grid Margin="10 0">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="*" />
                                        </Grid.RowDefinitions>
                                        <TextBlock Grid.Column="1" Name="region" FontSize="18"/>
                                        <TextBlock Grid.Column="1" Grid.Row="1" Name="cloud" FontSize="18"/>
                                        <TextBlock Grid.Column="3" Grid.Row="1" Name="temp" FontSize="18"/>
                                        <TextBlock Grid.Column="3" Name="wind" FontSize="18"/>
                                        <TextBlock FontSize="18" Text="Туман :"/>
                                        <TextBlock Text="Шамол :" Grid.Column="2" FontSize="18"/>
                                        <TextBlock Text="Булутлилик :" Grid.Row="1" FontSize="18"/>
                                        <TextBlock Text="Ҳарорат :" Grid.Column="2" Grid.Row="1" FontSize="18"/>
                                    </Grid>
                                </Border>
                                <TextBlock Grid.Column="5" x:Name="userName" Text="Бузруков Жавоҳир " FontSize="20" HorizontalAlignment="Right" VerticalAlignment="Center"/>
                                <materialDesign:PopupBox  Grid.Column="6"
                                     DockPanel.Dock="Right"
                                     PlacementMode="BottomAndAlignRightEdges">
                                    <ListBox>
                                        <ListBoxItem>
                                            <Button x:Name="btnExitAccount" Content="Чиқиш" Click="btnExitAccount_Click"/>
                                        </ListBoxItem>
                                    </ListBox>
                                </materialDesign:PopupBox>
                            </Grid>
                        </materialDesign:ColorZone>
                    </Border>
                    <!--Navbar end-->
                    <Grid Grid.Row="1" Margin="0,15,0,15">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="Auto"/>
                        </Grid.ColumnDefinitions>
                        <Grid Grid.Column="0">
                            <Grid.Resources>
                                <Style TargetType="Button">
                                    <Setter Property="Background" Value="#2196F3"/>
                                    <Setter Property="Padding" Value="10,10"/>
                                    <Setter Property="Margin" Value="10"/>
                                    <Setter Property="FontSize" Value="25"/>
                                    <Setter Property="Foreground" Value="White"/>
                                    <Setter Property="FontWeight" Value="SemiBold"/>
                                    <Setter Property="BorderThickness" Value="0"/>
                                    <Style.Resources>
                                        <Style TargetType="Border">
                                            <Setter Property="CornerRadius" Value="10" />
                                        </Style>
                                    </Style.Resources>
                                </Style>
                            </Grid.Resources>
                            <Border Background="#B3E5FC" 
                                CornerRadius="0 20 20 0" x:Name="MenuPanel" Padding="15,30,15,30" Margin="3,0,5,0" >
                                <StackPanel Orientation="Vertical" Name="dropdownInnerContent">
                                    <Button HorizontalAlignment="Left" Width="315" Name="btnOpenInterfaceSettingsDialog" Click="btnOpenInterfaceSettingsDialog_Click">
                                        <Button.Effect>
                                            <DropShadowEffect>
                                                <DropShadowEffect.BlurRadius>10</DropShadowEffect.BlurRadius>
                                                <DropShadowEffect.Opacity>0.3</DropShadowEffect.Opacity>
                                                <DropShadowEffect.Color>#2196F3</DropShadowEffect.Color>
                                            </DropShadowEffect>
                                        </Button.Effect>
                                        Интерфейс созламалари
                                    </Button>
                                    <Button HorizontalAlignment="Left" Width="315" Name="btnOpenAddGateDialog">
                                        <Button.Effect>
                                            <DropShadowEffect>
                                                <DropShadowEffect.BlurRadius>10</DropShadowEffect.BlurRadius>
                                                <DropShadowEffect.Opacity>0.3</DropShadowEffect.Opacity>
                                                <DropShadowEffect.Color>#2196F3</DropShadowEffect.Color>
                                            </DropShadowEffect>
                                        </Button.Effect>
                                        Янги дарвоза яратиш
                                    </Button>
                                    <Button HorizontalAlignment="Left" Width="315" Name="btnOpenGateSettingsDialog" >
                                        <Button.Effect>
                                            <DropShadowEffect>
                                                <DropShadowEffect.BlurRadius>10</DropShadowEffect.BlurRadius>
                                                <DropShadowEffect.Opacity>0.3</DropShadowEffect.Opacity>
                                                <DropShadowEffect.Color>#2196F3</DropShadowEffect.Color>
                                            </DropShadowEffect>
                                        </Button.Effect>
                                        Дарвозани созлаш
                                    </Button>
                                    <Button VerticalAlignment="Center" HorizontalAlignment="Left" Width="315" Name="btnOpenLicenseDialog">
                                        <Button.Effect>
                                            <DropShadowEffect>
                                                <DropShadowEffect.BlurRadius>10</DropShadowEffect.BlurRadius>
                                                <DropShadowEffect.Opacity>0.3</DropShadowEffect.Opacity>
                                                <DropShadowEffect.Color>#2196F3</DropShadowEffect.Color>
                                            </DropShadowEffect>
                                        </Button.Effect>
                                        Лицензия
                                    </Button>
                                    <Button x:Name="btnAddUser" HorizontalAlignment="Left" Width="315">
                                        <Button.Effect>
                                            <DropShadowEffect>
                                                <DropShadowEffect.BlurRadius>10</DropShadowEffect.BlurRadius>
                                                <DropShadowEffect.Opacity>0.3</DropShadowEffect.Opacity>
                                                <DropShadowEffect.Color>#2196F3</DropShadowEffect.Color>
                                            </DropShadowEffect>
                                        </Button.Effect>
                                        Фойдаланувчи қўшиш
                                    </Button>
                                    <Button x:Name="btnOpenLoginSmartWaterDialog" HorizontalAlignment="Left" Width="315">
                                        <Button.Effect>
                                            <DropShadowEffect>
                                                <DropShadowEffect.BlurRadius>10</DropShadowEffect.BlurRadius>
                                                <DropShadowEffect.Opacity>0.3</DropShadowEffect.Opacity>
                                                <DropShadowEffect.Color>#2196F3</DropShadowEffect.Color>
                                            </DropShadowEffect>
                                        </Button.Effect>
                                        Connect SmartWater
                                    </Button>
                                    <Button x:Name="btnSaveDatabase" HorizontalAlignment="Left" Width="315">
                                        <Button.Effect>
                                            <DropShadowEffect>
                                                <DropShadowEffect.BlurRadius>10</DropShadowEffect.BlurRadius>
                                                <DropShadowEffect.Opacity>0.3</DropShadowEffect.Opacity>
                                                <DropShadowEffect.Color>#2196F3</DropShadowEffect.Color>
                                            </DropShadowEffect>
                                        </Button.Effect>
                                        Базани сақлаш
                                    </Button>
                                </StackPanel>
                            </Border>
                        </Grid>
